{
  "name": "react-vite",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "pnpm --parallel --filter \"./**\" build",
    "serve": "pnpm --parallel --filter \"./**\" preview",
    "build:remotes": "pnpm --parallel --filter \"./react-vite-leaflet\" build",
    "serve:remotes": "pnpm --parallel --filter \"./react-vite-leaflet\" serve",
    "dev:hosts": "pnpm --filter \"./host\" dev",
    "stop": "kill-port --port 5000,5001"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.4",
    "kill-port": "^2.0.1"
  },
  "dependencies": {}
}